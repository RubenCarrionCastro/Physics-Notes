 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          StyleBox["H", "TI"], "\[Prime]"], "\[LongEqual]", 
         FractionBox["1", 
          SubscriptBox[
           StyleBox["r", "TI"], "12"]], "\[TildeTilde]", 
         FractionBox["1", "radio atomico"], "\[TildeTilde]", "1", 
         " Hartree"}], TraditionalForm], "errors" -> {}, "input" -> 
     "H'=\\frac{1}{r_{12}}\\approx\\frac{1}{\\text{radio \
atomico}}\\approx1\\text{ Hartree}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "29bc9399-9f3f-e84e-9642-dd1f28b47a08"],
